#!/bin/bash -ex

    #Leaf1 rules
    sudo iptables -t nat -I PREROUTING 1 -d 10.17.161.200 -p tcp --dport 10100 -j DNAT --to-destination 192.168.121.171:22
    sudo iptables -I FORWARD 1 -p tcp -d 192.168.121.171 --dport 22 -m state --state NEW,ESTABLISHED,RELATED -j ACCEPT
    sudo iptables -t nat -I POSTROUTING 1 -s 192.168.121.171 -j SNAT --to-source 10.17.161.200

